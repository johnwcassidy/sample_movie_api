{
  "functions": {
    "predeploy": ["npm --prefix \"$RESOURCE_DIR\" run lint", "npm --prefix \"$RESOURCE_DIR\" run build"]
  },
  "hosting": {
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/**",
        "function": "main"
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "https://hub.apitree.com/johnwcassidy/sample-movie-api/",
        "type": 301
      }
    ]
  }
}
